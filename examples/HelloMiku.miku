song HelloMiku {
    let message = "Hello, World! ðŸŽµ"
    let tempo = 120
    let tralala: note // default to zero

    melody greet(name: lyric) -> note {
        sing "Greetings ", name, " ! ðŸ’™"
        tune song.tempo up 10
        crescendo song.byebye() * song.tempo
    }

    melody byebye() -> note {
        crescendo 42
    }

    melody type() -> harmony {
        let test: harmony = { "name": 1, "key": true, "lol": 3 }
        crescendo test
    }

    melody character() -> harmony {
        crescendo { "name": "Teto", "surname": "Kasane" }
    }

    melody index(i: note) {
        sing "index note: ", i
    }

    melody index(i: lyric) {
        sing "index lyric: ", i
    }

    melody index(i: lyric, value: note) {
        sing "set index lyric: ", i, " to value ", value
    }

    verse main(lyrics: track) -> note {
        let hello = play HelloMiku
        sing hello.message
        if hello.tempo > 100 drops {
            repeat 3 times {
                sing "â™ª La la la â™«"
            }
        } else if hello.tempo < 100 drops {
            sing "bar"
        } else {
            sing "foo"
            crescendo 0
        }
        sing "name:"
        sing hello.type().name

        sing hello.greet(hello.character().name)

        let n = 1
        while n <= 5 sounds {
            sing n, " times"
            n = n + 1
        }

        countdown dog from 3 {
            sing dog, " dogs"
        }

        for c in [1337, 69, 42, 666] {
            sing c
        }

        sing "1337: ", [1337, 69, 42, 666][0]
        sing "69: ", [1337, 69, 42, 666][1]
        sing "track.len(): ", [1337, 69, 42, 666].len()

        hello[0]
        hello["hello"] = 42

        sing hello.tralala

        sing "end"

        crescendo 0
    }
}
